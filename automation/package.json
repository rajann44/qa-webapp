{
    "scripts": {
        "start": "cypress install",
        "cypress:open": "cypress open",
        "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
        "pretest": "npm run clean:reports",
        "scripts": "cypress run -r cypress-multi-reporters test_script --reporter mochawesome",
        "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
        "merge-report":"mochawesome-merge  cypress/reports/mocha/*.json> cypress/reports/report.json",
        "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mocha",
        "posttest": "npm run combine-reports && npm run generate-report",
        "test" : "npm run scripts || npm run posttest"
    },
    "devDependencies": {
        "cypress-multi-reporters": "^1.5.0",
        "mocha": "^9.2.2",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^4.2.1",
        "mochawesome-report-generator": "^6.2.0"
    }
}
